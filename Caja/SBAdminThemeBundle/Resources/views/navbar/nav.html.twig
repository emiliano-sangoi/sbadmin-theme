<nav class="sb-topnav navbar navbar-expand navbar-{{ color|default('primary') }} bg-{{ color|default('primary') }}" id="navbar">
    <!-- Navbar Brand-->
    <a class="navbar-brand ps-3" href="https://santafe.gob.ar/" target="_blank">
        {% block navbar_logo %}
            <img src="{{ bt_logo_gob_stafe|default('') }}" class="img-fluid" alt="Santa Fe">
        {% endblock %}
        <div class="d-inline-block pl-lg-3">
            <div class="app-name text-primary fst-italic ">
                {% block navbar_app_name %}
                    <h4 class="d-inline-block mb-0 fw-bold">
                            {{ (bt_app_nombre|default('app'))|upper }}
                    </h4>
                {% endblock %}
                <small class="fs-6">
                    {% block navbar_app_version %}
                        {{ bt_app_version|default('v0.0') }}
                    {% endblock %}
                </small>
            </div>
        </div>
    </a>
    <!-- Sidebar Toggle-->
    <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0 text-secondary" id="sidebarToggle" href="#!">
        <i class="fas fa-bars"></i>
    </button>



    <!-- Navbar Search-->
