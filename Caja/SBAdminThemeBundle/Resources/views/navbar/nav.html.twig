{#            </div>#}
{#        </form>#}
    {% endblock %}

    <!-- Navbar-->
    <ul class="navbar-nav ms-auto me-0 me-md-3 my-2 my-md-0">
        {% block navItems %}
            {% if nav_items_file is defined %}
                {% include nav_items_file %}
            {% else %}
                {% include '@CajaSBAdminTheme/navbar/nav-items.html.twig' %}
            {% endif%}
        {% endblock %}
    </ul>
</nav>