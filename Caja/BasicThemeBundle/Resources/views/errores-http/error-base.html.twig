<div class="">

    {% embed "@CajaBasicTheme/partials/card-basica.html.twig" with { 'codigo_ayuda' : '', 'card_header_content' : 'Informaci√≥n del error' } %}
        {% block card_body %}
            <div>
                {{ msg_error_html }}
            </div>

            {% if show_context_info|default(true) %}
                <div class='mt-3'>
                    {% include '@CajaBasicTheme/errores-http/context-info.html.twig' %}
                </div>
            {% endif %}

            <div class='mt-3'>    
                {% embed '@CajaBasicTheme/partials/botonera.html.twig' %}

                    {% from '@CajaBasicTheme/macros.html.twig' import btn_primary, btn_link %}

                    {% block left %}
                        <a href='{{ target_uri|default('#') }}' class='btn btn-primary'>
                            {#<i class='bi-house-fill'></i>&nbsp;#}
                            {{ target_label|default('Ir al inicio') }}
                        </a>                                     
                    {% endblock %}

                {% endembed %}
            </div>    

        {% endblock %}
    {% endembed %}
</div>



